{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 3,
    "name": "TortillaReps Agent Warning Removal Tool",
    "version": "1.3",
    "description": "Plugin to remove Warm Reminder",
	"homepage_url": "https://github.com/socram25/WarmWarningRemoval",
	"author": "TortillaReps",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://*.cnfans.com/*",
        "https://*.mulebuy.com/*",
        "https://*.hoobuy.com/*",
        "https://*.hagobuy.com/*",
        "https://*.hegobuy.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.cnfans.com/*",
                "https://*.mulebuy.com/*",
                "https://*.hoobuy.com/*",
                "https://*.hagobuy.com/*",
                "https://*.hegobuy.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
