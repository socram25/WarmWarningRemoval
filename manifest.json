{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 3,
    "name": "Agent Warning Removal Tool",
    "version": "1.5",
    "description": "Extension to remove Warm Reminder",
	"homepage_url": "https://github.com/socram25/WarmWarningRemoval",
	"author": "TortillaReps",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://*.cnfans.com/*",
        "https://*.mulebuy.com/*",
        "https://*.hoobuy.com/*"
        
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "16px.png",
            "48": "48px.png",
            "128": "128px.png"
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "48px.png",
        "128": "128px.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.cnfans.com/*",
                "https://*.mulebuy.com/*",
                "https://*.hoobuy.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
